searched_book = input()
counter = 0
next_book = input()
isFound = False
while next_book != "No More Books":
    if next_book == searched_book:
        isFound = True
        break
    next_book = input()
    counter += 1
if isFound:
    print(f"You checked {counter} books and found it.")
else:
    print("The book you search is not here!")
    print(f"You checked {counter} books.")
