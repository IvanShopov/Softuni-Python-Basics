first_time = int(input())
second_time = int(input())
third_time = int(input())
total_time_in_seconds = first_time + second_time + third_time
total_time_minutes = total_time_in_seconds // 60
left_seconds = total_time_in_seconds - total_time_minutes * 60
if left_seconds < 10:
    print(f"{total_time_minutes}:0{left_seconds}")
else:
    print(f"{total_time_minutes}:{left_seconds}")
